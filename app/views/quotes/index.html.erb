<%= turbo_stream_from(current_company, "quotes") %>
<main>
  <div>
    <h1>Quotes</h1>
    <%= link_to "New quote", new_quote_path, data: {turbo_frame: dom_id(Quote.new)} %>
  </div>
  <%= turbo_frame_tag dom_id(Quote.new) %>
  <%= turbo_frame_tag "quotes" do %>
    <%= render @quotes %>
  <% end %>
</main>
