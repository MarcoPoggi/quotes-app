<%= turbo_stream_from(current_company, "quotes") %>
<main class="quotes_container">
  <header class="quotes_container--header">
    <h1>Quotes</h1>
    <%= link_to "New quote", new_quote_path, data: {turbo_frame: dom_id(Quote.new)}, class: "button-link"%>
  </header>
  <%= turbo_frame_tag dom_id(Quote.new)%>
  <%= turbo_frame_tag "quotes" do %>
    <section class="quotes_container--content" id="quotes-container">
      <%= render "quotes/empty_state" %>
      <%= render @quotes %>
    </section>
  <% end %>
</main>
